<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="textview_text_item1">
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:textColor">@color/textview</item>
    </style>
    
    <style name="textview_text_item0">
        <item name="android:textSize">@dimen/textsize_min</item>
        <item name="android:textColor">@color/textview</item>
    </style>
    
    <style name="textview_type_item0">
        <item name="android:textSize">@dimen/textsize_min</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:background">@drawable/yj_gv2_shape</item>
    </style>

    <style name="textview_bold_item">
        <item name="android:textSize">@dimen/text_size_16</item>
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="imageview_item">
        <item name="android:width">32dp</item>
        <item name="android:height">16dp</item>
    </style>
    
    <style name="linearlayout_h0">
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">4dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- gridview -->
    <style name="include_gvst">
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_margin">@dimen/gridview_margin</item>
    </style>
    
    <style name="include_gv0">
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">@dimen/actionbar_margin_less</item>
    </style>
    
    
    
</resources>